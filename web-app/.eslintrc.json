{
    "env": {
      "browser": true
    },
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "off"
      ],
      "no-mixed-operators": [
        "off"
      ],
      "no-param-reassign": [
        "off"
      ],
      "react/prop-types": [
        "off"
      ],
      "react/jsx-closing-bracket-location": [
        "off"
      ],
      "react/no-array-index-key": [
        "off"
      ],
      "jsx-a11y/no-static-element-interactions": [
        "off"
      ]
    }
}
